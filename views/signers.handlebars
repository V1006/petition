<nav id="nav">
    <ul>
        <div class="lis">
            <li><a href="/petition/profile/edit"><i
                    >{{currentUser.first_name}}</i>
                    |</a>
            </li>
            <li><a href="/petition/logout">Logout |</a></li>
        </div>
    </ul>
</nav>
<h1>These are our {{allUserData.length}} Signers</h1>
<ul class="signers">
    {{#each allUserData}}
        {{#if url}}
            <div class="liBox">
                <li>Name: <p><a
                        target="_blank"
                        href="{{url}}"
                    >{{first_name}} {{last_name}}</a></p><p>Age: {{age}}
                        <p>City: <a
                        href="/petition/signers/{{city}}"
                    >{{city}}</a></p></li>
            </div>
        {{else}}
            <div class="liBox">
                <li>Name: 
                    <p>{{first_name}} {{last_name}}</p>
                    <p>Age: 
                    {{age}}</p>
                    <p>City: <a href="/petition/signers/{{city}}">{{city}}</a></p></li>
            </div>
        {{/if}}
    {{/each}}
</ul>
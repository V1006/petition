<section>
    <h1>Petition</h1>
    <form
        class="userData"
        id="register"
        action="/petition/register"
        method="POST"
    >
        <p>First Name</p><input
            type="text"
            value=""
            name="first_name"
            placeholder="First Name"
            required
        />
        <p>Last Name</p><input
            type="text"
            value=""
            name="last_name"
            placeholder="Last Name"
            required
        />
        <p>E-mail</p><input
            type="email"
            value=""
            name="email"
            placeholder="E-mail"
            required
        />
        <p>Password</p><input
            type="password"
            value=""
            name="password"
            placeholder="password"
            required
        />
    </form>
    <div class="signHere">
        {{#if error}}
            <p class="error">Fill out all fields</p>
        {{/if}}
        <button id="submit" type="submit" form="register">Register</button>

    </div>

</section>